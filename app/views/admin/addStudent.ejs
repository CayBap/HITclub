<!DOCTYPE html>
<html lang="en">

<head>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">

    <title>Thêm sinh viên</title>

    <!-- Bootstrap Core CSS -->

    <link href="/static/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">

    <!-- MetisMenu CSS -->
    <link href="/static/vendor/metisMenu/metisMenu.min.css" rel="stylesheet">

    <!-- Custom CSS -->
    <link href="/static/dist/css/sb-admin-2.css" rel="stylesheet">

    <!-- Custom Fonts -->
    <link href="/static/vendor/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css">

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->

</head>

<body>
    <div id="wrapper">
        <%- include ../layout/nav.ejs %>
            <div id="page-wrapper" style="min-height: 355px;">
                <div class="row">
                    <div class="col-lg-12">
                        <h1 class="page-header">Thêm sinh viên</h1>
                    </div>
                    <!-- /.col-lg-12 -->
                </div>
                <!-- /.row -->
                <div class="row">
                    <div class="col-xs-6 col-sm-6 col-md-6 col-lg-6">

                        <form action="" method="POST">
                                <%if(data && data.error){%>
                                    <div  id="login-alert" class="alert alert-danger col-sm-12">
                                        <%=data.error%>
                                    </div>
                                    <%}%>
                            <div class="form-group has-success">
                                <label class="control-label" for="inputSuccess">Họ tên:</label>
                                <input type="text" name="name" class="form-control" id="inputSuccess">
                            </div>
                            <div class="form-group has-success">
                                <label class="control-label" for="inputSuccess">Lớp-khóa:</label>
                                <input type="text" name="class" class="form-control" id="inputSuccess">
                            </div>
                            <div class="form-group has-success">
                                <label class="control-label" for="inputSuccess">Mã sinh viên:</label>
                                <input type="number" name="idStudent" class="form-control" id="inputSuccess">
                            </div>
                            <div class="form-group has-success">
                                <label class="control-label" for="inputSuccess">Số điện thoại:</label>
                                <input type="number" name="phoneNumber" class="form-control" id="inputSuccess">
                            </div>
                            <div class="form-group has-success">
                                <label class="control-label" for="inputSuccess">Ngày tháng năm sinh:</label>
                                <input type="date" name="birthday" class="form-control" id="inputSuccess">
                            </div>
                            <div class="form-group has-success">
                                <label class="control-label" for="inputSuccess">Quê quán:</label>
                                <input type="text" name="where" class="form-control" id="inputSuccess">
                            </div>
                            <button type="submit" class="btn btn-outline btn-success">Thêm sinh viên</button>
                        </form>
                    </div>



                    <div class="col-xs-1 col-sm-1 col-md-1 col-lg-1">

                    </div>

                    <div class="col-xs-5 col-sm-5 col-md-5 col-lg-5">
                        <div>

                        </div>
                    </div>



                </div>
                <!-- /.row -->
            </div>


            <!-- jQuery -->
            <script src="/static/vendor/jquery/jquery.min.js"></script>

            <!-- Bootstrap Core JavaScript -->
            <script src="/static/vendor/bootstrap/js/bootstrap.min.js"></script>

            <!-- Metis Menu Plugin JavaScript -->
            <script src="/static/vendor/metisMenu/metisMenu.min.js"></script>

            <!-- Custom Theme JavaScript -->
            <script src="/static/dist/js/sb-admin-2.js"></script>

</body>

</html>